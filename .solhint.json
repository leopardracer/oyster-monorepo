{
    "extends": "solhint:recommended",
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "warn",
        "const-name-snakecase": "off",
        "avoid-suicide": "error",
        "avoid-sha3": "warn",
        "not-rely-on-time": "warn",
        "not-rely-on-block-hash": "warn"
    }
}